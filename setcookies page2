  <script type="text/javascript">
    $(window).on('load', function() {
      // Offset nav element
      $('#nav').affix({
        offset: 155
      });

      // add padding element above is affixed
      $('#nav').on('affix.bs.affix', function() {
        $('#content').addClass('padded');
      });

      // and remove again when unaffixed
      $('#nav').on('affix-top.bs.affix', function() {
        $('#content').removeClass('padded');
      });

      // refresh parent window on modal close
      $('#my-modal').on('hidden.bs.modal', function() {
        location.reload();
      });

    });

    function processFeed() {
        // feed to parse
        var feed = "https://www.getrave.com/rss/bc/channel2";
        $.ajax(feed, {
            accepts:{
                xml:"application/rss+xml"
            },
            dataType:"xml",
            success:function(data) {
                console.log("success");
                $(data).find("item").each(function() {
                    var el = $(this);
                    var description = el.find("description").text();
                    if (description  != 'inactive') {
                      console.log("active alert");
                      $('#text').text(description);
                      $('#emergency').addClass('appear');
                      $('#emergency').css('display','block');
                    }
                    else {
                      console.log("inactive alert." + description);
                      $('#emergency').css('display','none');
                      $('#emergency').removeClass('appear');
                      $('#text').html('');
                    }
                });
            },
            complete: setTimeout(function() {processFeed()}, 30000),
            timeout: 2000
        });
    };

    $(function() {
      processFeed();
    });

  </script>


  
  <link type="text/css" rel="stylesheet" href="https://services.bc.edu/assets/css/my-services.css" />
  <script type="text/javascript">
    $(window).on('load', function() {
      // capitalize "Food"
      $('.panel-heading').each(function() {
        var text = $(this).text();
        text = text.replace('food','Food');
        $(this).text(text);
      });
    });
    $(function() {
      // make clickable each parent <li> that contains a service link
      $('#all-services li:not(.disabled)').click(function() {
        var url = $(this).find('a:first');
        if (url.attr('target') == '_blank') {
          window.open(url.attr('href'));
        }
        else {
          window.location = url.attr('href');
        }
        return false;
      });
      // tab focus each service link
      $('#all-services a').on('focusin',function() {
        $(this).parent('li').removeClass('bkgrd-off');
        $(this).parent('li').addClass('bkgrd-on');
      }).on('focusout',function() {
        $(this).parent('li').removeClass('bkgrd-on');
        $(this).parent('li').addClass('bkgrd-off');
      });
      // blur each service link on hover
      $('#all-services .panel-body').hover(function() {
        $('a').each(function() {
          $(this).blur();
        });
      });
      // append list item within services region with accessibility tip
      $('#all-services .disabled').append('<span class="sr-only">Service is unavailable</span>');
    });
  </script>
